package ListaDeExercicios_1;

public class Pais {

	private String nome;
	private String capital;
	private double km;
	private String Fronteira[];
	
	Pais(String nome, String capital, double km, String Fronteira[]){
		this.setNome(nome);
		this.setCapital(capital);
		this.setKm(km);
		this.setFronteira(Fronteira[]);
	}
	
	public boolean equals (Pais outro) {
		if(getNome() == outro.getNome() && getCapital() == outro.getCapital()) {
			return true;
		}else {
			return false;
		}
	}
	
	

	//GETS AND SETS
	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public String getCapital() {
		return capital;
	}

	public void setCapital(String capital) {
		this.capital = capital;
	}

	public double getKm() {
		return km;
	}

	public void setKm(double km) {
		this.km = km;
	}

	public String[] getFronteira() {
		return Fronteira;
	}

	public void setFronteira(String fronteira[]) {
        this.Fronteira[] = fronteira[];
	}
}
